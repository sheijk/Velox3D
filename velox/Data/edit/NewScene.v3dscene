	</entity>
	<entity name="coordsys">
		<part type="v3d::scene::VModelPart" material="" mesh="" model="/data/afighter.3ds"/>
		<part type="v3d::entity::VRigidBodyPart" pos="(0,0,-20)"/>
	</entity>
	<entity name="lantern">
		<part type="v3d::entity::VRigidBodyPart" pos="(0,0,0)"/>
		<part type="v3d::scene::VModelPart" material="" mesh="" model="/data/streetlight.3ds"/>
	</entity>
